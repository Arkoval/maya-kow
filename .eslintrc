{
    "extends": ["prettier", "prettier/react"],
    "env": {
      "jest": true
    },
    "globals": {
      "document": true
    },
  
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"],
          "moduleDirectory": ["node_modules", "src/"]
        }
      }
    }
  }
  